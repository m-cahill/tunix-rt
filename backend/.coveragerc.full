[run]
branch = True
source = tunix_rt_backend
# No omit - measure everything including optional UNGAR code

[report]
# Report-only mode for optional workflow (no fail_under)
exclude_lines =
    pragma: no cover
    def __repr__
    if __name__ == .__main__.:
    raise AssertionError
    raise NotImplementedError
    if TYPE_CHECKING:
    @abstractmethod
